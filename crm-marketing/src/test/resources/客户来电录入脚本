--将回访录入查询更改为营销录入查询
--影响行数一行
update t_auth_function t set t.ffunctionname = '营销录入查询' where t.ffunctionname = '回访录入查询';

--货量潜力表添加索引
create index IDX_PARENTID_POTENTIAL on T_CUST_RETURNVISITDETAIL (FPARENTID);

--客户意见表添加索引
create index IDX_PARENTID_OPTION on T_CUST_REVISITOPINIONDETAIL (FPARENTID)

